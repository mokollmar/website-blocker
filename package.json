{
  "name": "website-blocker",
  "displayName": "Website Blocker",
  "version": "1.0.1",
  "description": "An open-source website blocker that lets you block any site of your choice.",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/svelte-fontawesome": "^0.2.2",
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.12.0",
    "plasmo": "0.89.1",
    "svelte": "^4.2.18",
    "svelte-preprocess": "^6.0.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "externally_connectable": {
      "matches": [
        "*://localhost/*",
        "*://mokollmar.com/*"
      ]
    },
    "permissions": [
      "activeTab"
    ]
  }
}